  {
    "name": "DKB-Online-Cashback-Reminder",
    "version": "1.0",
    "description": "Show a reminder if the current site is part of the DKB-Online-Cashback Program ",
    "permissions": [
      "storage",
      "tabs",
      "activeTab",
      "declarativeContent",
      "https://www.dkb.de/banking/plus/online-cashback/",
      "https://www.dkb.de/Welcome/content/CmsDetail/Card4YouShops.xhtml"
    ],
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "images/percentage16.png",
        "32": "images/percentage32.png",
        "48": "images/percentage48.png",
        "128": "images/percentage128.png"
      }
    },
    "icons": {
      "16": "images/percentage16.png",
      "32": "images/percentage32.png",
      "48": "images/percentage48.png",
      "128": "images/percentage128.png"
    },
    "options_page": "options/options.html",
    "manifest_version": 2
  }